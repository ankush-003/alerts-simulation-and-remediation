{"_id":{"$oid":"65f2e3d1fff6e1ee2b5074af"},"name":"Rule6","desc":"Memory Limit check Critical","salience":{"$numberInt":"10"},"when":"InputData.Type == \"Memory\" && InputData.Severity == \"Critical\"","then":["OutputData.Remedy = \"Stop all processes\"","InputData.Severity = \"Handled\""]},
{"_id":{"$oid":"65f810b585a9cf2863a680ae"},"name":"Rule7","desc":"Memory Limit check Severe","salience":{"$numberInt":"10"},"when":"InputData.Type == \"Memory\" && InputData.Severity == \"Severe\"","then":["OutputData.Remedy = \"Stop some processes\"","InputData.Severity = \"Handled\""]},
{"_id":{"$oid":"65fbc82c51cac2ce34617a50"},"name":"Rule5","desc":"Configured max number of file descriptors is too low (< 256)","salience":{"$numberInt":"10"},"when":"InputData.Type == \"Computer Generic\" && InputData.Severity == \"Critical\"","then":["OutputData.Remedy = \"Adjust file descriptor settings.\"","InputData.Severity = \"Handled\""]},
{"_id":{"$oid":"65fbc99051cac2ce34617a53"},"name":"Rule4","desc":"Interface eth0 has changed to lower speed than it was before","salience":{"$numberInt":"10"},"when":"InputData.Type == \"Network Interface\" && InputData.Severity == \"Information\"","then":["OutputData.Remediation = \"No immediate action required.\"","InputData.Severity = \"Handled\""]},
{"_id":{"$oid":"65fc0cfdf18da5a2b90d5c17"},"name":"Rule3","desc":"Configured max number of file descriptors is too low (< 512)","salience":{"$numberInt":"10"},"when":"InputData.Type == \"Computer Generic\" && InputData.Severity == \"Warning\"","then":["OutputData.Remedy = \"Adjust file descriptor settings if necessary\"","InputData.Severity = \"Handled Warning\""]}
